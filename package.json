{
  "name": "tauri-butt",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:ui": "vite build",
    "preview": "vite preview",
    "build:peer": "npm run build:peer:bundle && npm run build:peer:pkg",
    "build:peer:bundle": "noderify peer.js > peer.bundle.js",
    "build:peer:pkg": "pkg package.json --output src-tauri/binaries/app --compress GZip && node scripts/move-binary.js",
    "tauri": "tauri",
    "build": "npm run tauri build"
  },
  "bin": "peer.bundle.js",
  "pkg": {
    "assets": [
      "node_modules/leveldown/**/*",
      "node_modules/leveldown/node_modules/node-gyp-build/**/*",
      "node_modules/sodium-native/**/*",
      "node_modules/node-gyp-build/**/*"
    ],
    "targets": [
      "node14-linux-x64"
    ]
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.5",
    "env-paths": "^2.2.1",
    "secret-stack": "^6.4.1",
    "ssb-caps": "^1.1.0",
    "ssb-db2": "^4.1.2",
    "ssb-keys": "^8.2.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.11",
    "@vitejs/plugin-vue": "^2.3.3",
    "execa": "^5.1.1",
    "noderify": "^5.1.0",
    "pkg": "^5.7.0",
    "vite": "^2.9.9"
  }
}
